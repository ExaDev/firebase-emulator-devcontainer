		"@storybook/blocks": "^7.0.27",
		"@storybook/svelte": "^7.0.27",
		"@storybook/sveltekit": "^7.0.27",
		"@storybook/testing-library": "^0.0.14-next.2",
		"@sveltejs/adapter-static": "^2.0.2",
